{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@cv":[
        "./cv.json"
      ],
      "@/*": [
				"src/*"
			],
			"@components/*": [
				"src/components/*"
			],
			"@assets/*": [
				"src/assets/*"
			],
			"@layouts/*": [
				"src/layouts/*"
			],
			"@css/*": [
				"src/styles/*"
			],
			"@lib/*": [
				"src/lib/*"
			]
    }

  }
}